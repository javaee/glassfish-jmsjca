<weblogic-ejb-jar xmlns="http://www.bea.com/ns/weblogic/90" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.bea.com/ns/weblogic/90 http://www.bea.com/ns/weblogic/90/weblogic-ejb-jar.xsd">
    <weblogic-enterprise-bean>
        <ejb-name>mdbtest</ejb-name>
        <message-driven-descriptor>
            <pool>
                <max-beans-in-free-pool>32</max-beans-in-free-pool>
                <initial-beans-in-free-pool>0</initial-beans-in-free-pool>
            </pool>
            <resource-adapter-jndi-name>ratest</resource-adapter-jndi-name>
        </message-driven-descriptor>
        <resource-env-description>
            <resource-env-ref-name>queuefact</resource-env-ref-name>
            <jndi-name>__SYSTEM/resource/ratest-test#ratest#javax.jms.QueueConnectionFactory</jndi-name>
        </resource-env-description>
        <resource-env-description>
            <resource-env-ref-name>topicfact</resource-env-ref-name>
            <jndi-name>__SYSTEM/resource/ratest-test#ratest#javax.jms.TopicConnectionFactory</jndi-name>
        </resource-env-description>
        <resource-env-description>
            <resource-env-ref-name>unifiedfact</resource-env-ref-name>
            <jndi-name>__SYSTEM/resource/ratest-test#ratest1#javax.jms.ConnectionFactory</jndi-name>
        </resource-env-description>
        <jndi-name>mdbtest</jndi-name>
    </weblogic-enterprise-bean>
</weblogic-ejb-jar>
